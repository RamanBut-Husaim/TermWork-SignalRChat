@using BSUIR.TermWork.ImageViewer.UI.WebSite.ViewModel.Friendship

@model System.Collections.Generic.IList<FriendViewModel>

<li id="friends-navbar-list" class="active">
    <a href="#">Friends</a>
    <ul class="nav">
    @for (int i = 0; i < Model.Count; ++i)
    {
        <li class="">
            <a href="#">
                <span data-key="@Model[i].Key" class="badge pull-right">0</span>
                @Model[i].FirstName @Model[i].LastName
            </a>
        </li>
    }
    </ul>
</li>