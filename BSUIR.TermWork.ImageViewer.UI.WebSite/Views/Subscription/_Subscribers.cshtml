@using BSUIR.TermWork.ImageViewer.UI.WebSite.ViewModel.Subscription;

@model IList<SubscriptionTargetViewModel>
           
<li id="subscription-targets-list" class="active">
    <a href="#">Subscribers</a>
    <ul class="nav">
        @for (int i = 0; i < Model.Count; ++i)
        {
            <li class="">
                <a href="@Url.Action("Index", "PersonalPage", new { key = Model[i].Key })">@Model[i].FirstName @Model[i].LastName <span class="badge pull-right">@Model[i].NewSubscriptionItems</span></a>
            </li>
        }
    </ul>
</li>
           

